{
  "name": "nyc-neighborhood-bucket-list",
  "version": "1.0.0",
  "description": "A full-stack MERN application for tracking NYC neighborhoods you've visited",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "install-all": "cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "nyc",
    "neighborhoods",
    "mern",
    "react",
    "typescript",
    "mongodb",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}